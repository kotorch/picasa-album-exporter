<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <startup useLegacyV2RuntimeActivationPolicy="true" xdt:Transform="Insert">
  </startup>
  <system.web>
    <httpModules xdt:Transform="Remove" />
    <compilation debug="false" targetFramework="4.0" xdt:Transform="Replace" />
    <httpRuntime requestValidationMode="2.0" xdt:Transform="Insert" />
  </system.web>
  <system.codedom>
    <compilers>
      <compiler>
        <providerOption name="CompilerVersion" value="v4.0" xdt:Transform="Replace" />
      </compiler>
    </compilers>
  </system.codedom>
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true" xdt:Transform="Insert">
      <add name="UrlRoutingModule" type="System.Web.Routing.UrlRoutingModule, System.Web, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />
    </modules>
  </system.webServer>
</configuration>
